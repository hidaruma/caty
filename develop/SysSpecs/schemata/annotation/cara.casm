// -*- coding: utf-8 -*-
/** caraで使うアノテーションの定義 
 */
module annotation.cara;

/** 動的なポートである
 * dynamicから変更
 */
@[annotation, target(["PORT_DECL"]) ]
type indirect = {};


/** 抽象状態である
 */
@[annotation, target(["STATE_DECL"]) ]
type abstract = {};


/** アクション核の入力境界となるフラグメントである
 */
@[annotation, target(["FRAGMENT_PROFILE_DECL"]) ]
type in = {};

/** アクション核の出力境界となるフラグメントである
 */
@[annotation, target(["FRAGMENT_PROFILE_DECL"]) ]
type out = {};


// End of Module


